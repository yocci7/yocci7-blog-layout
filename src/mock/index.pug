doctype html
html
  head
    title Yocci Blog
    link(rel='stylesheet' href='./common/css/app.css')
    script(src='./common/js/script.js')
    script(src='https://kit.fontawesome.com/7358297ab3.js' crossorigin='anonymous')
  body
    header.header
      nav.header__nav
        h2.header__nav__logo
          a(href='./index.html') YOCCI_ii BLOG
        ul.header__nav__ul
          li.header__nav__ul__li
            a(href='#') About
          li.header__nav__ul__li
            a(href='#') Index
          li.header__nav__ul__li
            a(href='#') Skil
          li.header__nav__ul__li
            a(href='#') Contact

    main.main
