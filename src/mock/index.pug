        .sidebar__search
          input.sidebar__search__input(placeholder='Search')
          .sidebar__search__button
            i.fa-solid.fa-magnifying-glass.fa-lg

        .sidebar__article__filter
          p.sidebar__article__filter__name Filter
          button.sidebar__article__filter__button(data-name='all') all
          button.sidebar__article__filter__button(data-name='programing') Programing
          button.sidebar__article__filter__button(data-name='minecraft') Minecraft
          button.sidebar__article__filter__button(data-name='other') other

